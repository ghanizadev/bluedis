{
  "name": "bluedis",
  "productName": "bluedis",
  "author": {
    "name": "Jean Felipe de Melo",
    "email": "jf.melo6@gmail.com"
  },
  "version": "0.2.1",
  "keywords": [
    "redis",
    "gui",
    "database",
    "bluedis",
    "management",
    "data",
    "visualization"
  ],
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "private": true,
  "main": "dist/main.js",
  "homepage": "./",
  "config": {
    "forge": "forge.config.js"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "ioredis": "^5.2.2",
    "nanoid": "^3.3.4",
    "update-electron-app": "^2.0.1"
  },
  "scripts": {
    "build": "npx gulp build",
    "build:electron": "tsc --project tsconfig.app.json",
    "build:react": "cross-env NODE_ENV=production CI=true BUILD_PATH=./build react-scripts build",
    "dev:electron": "cross-env NODE_ENV=development electron-forge start",
    "dev:react": "cross-env NODE_ENV=development BROWSER=none react-scripts start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "make": "electron-forge make",
    "pack": "npx gulp pack",
    "package": "electron-forge package",
    "predev:electron": "npm run build:electron",
    "prepare": "husky install",
    "start": "electron-forge start",
    "start:dev": "npx gulp start-dev",
    "start:prod": "npx gulp start-prod",
    "test:app": "jest --config app/jest.config.js --watchAll=false --runInBand",
    "test:react": "react-scripts test --watchAll=false --runInBand"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/core": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-snap": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-wix": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.25",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cross-env": "^7.0.3",
    "electron": "^18.3.7",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-localstorage-mock": "^2.4.21",
    "jest-styled-components": "^7.0.8",
    "modclean": "^3.0.0-beta.1",
    "prettier": "2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "redis-memory-server": "^0.5.0",
    "redux-mock-store": "^1.5.4",
    "styled-components": "^5.3.5",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  }
}
